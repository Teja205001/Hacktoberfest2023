#include<stdio.h>
main()
{
	int a[3][3],b[3][3],c[3][3];
	int i,j;
	printf("enter the first array elements:\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	 printf("array elements:\n");
	 for(i=0;i<3;i++)
	 {
	 	printf("\n");
	 	for(j=0;j<3;j++)
	 	{
	 		printf("%d\t",a[i][j]);
		 }
	 }
	 printf("\n");
	 printf("enter second array elements:");
	 for(i=0;i<3;i++)
	 {
	 	for(j=0;j<3;j++)
	 	{
	 		scanf("%d",&b[i][j]);
		 }
	 }
	 printf("array elements of 2nd array:\n");
	 for(i=0;i<3;i++)
	 {
	 	printf("\n");
	 	for(j=0;j<3;j++)
	 	{
	 		printf("%d\t",b[i][j]);
		 }
	 }
	printf("\n");
	printf("sum is");
	for(i=0;i<3;i++)
	{
		printf("\n");
	   	for(j=0;j<3;j++)
	   	{
	   		c[i][j]=a[i][j]+b[i][j];
	   		printf("%d\t",c[i][j]);
    	}
	   
	}
	
	
}
